			<key>name</key>
			<string>entity.other.attribute-name.tf2cfg</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>alias</string>
			<key>match</key>
			<string>\balias\b</string>
			<key>name</key>
			<string>entity.name.function.tf2cfg</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>binds</string>
			<key>match</key>
