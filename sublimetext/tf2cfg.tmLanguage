			<key>name</key>
			<string>keyword.tf2cfg</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>comment</string>
			<key>match</key>
			<string>//.*$</string>
			<key>name</key>
			<string>comment.tf2cfg</string>
